    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "git-commit-msg-linter": "^5.0.6",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
